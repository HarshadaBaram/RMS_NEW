@{ ViewData["Title"] = "Home Page";
}
@model RMS_NEW.Models.Ad_login
<div class="container">
    <div class="row">
        <div class="col-sm-4"></div>
        <div class="col-sm-4">
            <form class="form-horizontal" asp-action="Index">
                <div class="form-group">
                    <label class="control-label">Admin ID</label>
                    <input asp-for="Admin_id" class="form-control" type="text" />
                    <span asp-validation-for="Admin_id" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label class="control-label">Password</label>
                    <input asp-for="Ad_Password" class="form-control" type="password" />
                    <span asp-validation-for="Ad_Password" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Submit" />
                </div>
            </form>
        </div>
        <div class="col-sm-4"></div>
    </div>
</div>
@{
    if (TempData["msg"] != null)
    {
        <script>
            alert('@TempData["msg"]')
        </script>
    }
}